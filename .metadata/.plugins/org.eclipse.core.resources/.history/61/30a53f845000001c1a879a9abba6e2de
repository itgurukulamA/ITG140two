package com;

import java.util.ArrayList;
import java.util.List;

public class Listt {
	
		public static void main (String args[])
	
		{
List<String> l1=new ArrayList<String>();
l1.add("Nandini");
l1.add("Srinivas");
l1.add("harsha");
l1.add("jayadhar");
l1.add("surya");
System.out.println("1"+l1);
List<String> l2=new ArrayList<String>();
l2.add("rambabu");
l2.add("varma");
l2.add("laxmi");
l2.add("chaitanya");
System.out.println("2"+l2);
l1.addAll(l2);
System.out.println("3"+l1);
l1.remove("surya");
System.out.println("4"+l1);
l1.retainAll(l2);
System.out.println("5"+l1);
l1.removeAll(l2);
System.out.println("6"+l1);
l2.get(1);
System.out.println("7"+l2);
int size=l2.size();
System.out.println(size);
l2.clear();
System.out.println("8"+l2);
	}

}
